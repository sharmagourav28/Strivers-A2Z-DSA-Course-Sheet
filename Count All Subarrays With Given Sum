int findAllSubarraysWithGivenSum(vector < int > & nums, int k) {
    // Write Your Code Here
    unordered_map<int,int> mpp;
        mpp[0]=1;
        int presum = 0,cnt=0;
        for(int i =0;i<nums.size();i++){
            presum +=nums[i];
            int rem = presum-k;
            cnt+=mpp[rem];
            mpp[presum]+=1;
        }
        return cnt;
}
