#include <iostream>
using namespace std;

int main()
{
	// Write your code here
	int n;
	cin >> n;
	int k = n;
	int sum = 0;
	while (n > 0)
	{
		int lst = n % 10;
		sum = sum + (lst * lst * lst);
		n = n / 10;
	}
	if (sum == k)
	{
		cout << "true" << endl;
	}
	else
	{
		cout << "false" << endl;
	}
}
